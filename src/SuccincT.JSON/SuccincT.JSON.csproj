<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <Version>3.0.0</Version>
    <PackageId>SuccincT.JSON</PackageId>
    <Product>SuccincT.JSON</Product>
    <Authors>David Arno</Authors>
    <Company>David Arno</Company>
    <Copyright>Copyright Â© David Arno, 2017</Copyright>
    <Description>Succinc&lt;T&gt; is a .NET library that adds a number of functional features to C#</Description>
    <RepositoryUrl>https://github.com/DavidArno/SuccincT</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <AssemblyName>SuccincT.JSON</AssemblyName>
    <RootNamespace>SuccincT.JSON</RootNamespace>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
    <PostBuildEvent>copy succinct.json.dll "$(SolutionDir)Artifacts\succinct.json.1.1.dll</PostBuildEvent>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="9.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SuccincT\SuccincT.csproj" />
  </ItemGroup>

</Project>