<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <PackageId>SuccincT</PackageId>
    <Product>SuccincT</Product>
    <Authors>David Arno</Authors>
  	<Copyright>Copyright © David Arno, 2017</Copyright>
    <Description>Succinc&lt;T&gt; is a .NET library that adds a number of functional features to C#</Description>
    <AssemblyName>SuccincT</AssemblyName>
    <RootNamespace>SuccincT</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageIconUrl>https://raw.githubusercontent.com/DavidArno/SuccincT/master/SuccincT.png</PackageIconUrl>
    <PackageTags>Partial application Discriminated unions Options Maybe Pattern matching cons functional</PackageTags>
    <RepositoryUrl>https://github.com/DavidArno/SuccincT</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>V2.3.0 introduces the Success&amp;lt;T&amp;gt; and Either&amp;lt;TLeft,TRight&amp;gt; types, a number of new features for options, wildcard matching on tuples and type-based matching for general types.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ValueTuple" Version="4.3.0" />
  </ItemGroup>
</Project>